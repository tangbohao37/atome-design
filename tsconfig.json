{
  "compilerOptions": {
    "rootDir": ".",
    "composite": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable", "ES2015"],
    "baseUrl": ".",
    "jsx": "react-jsx",
    "allowJs": true,
    "types": ["node"],
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "./.vitepress/cache",
    "./.vitepress/dist"
  ],
  "include": ["./.vitepress/**/*", "./site/**/*", "./components/**/*"]
}
