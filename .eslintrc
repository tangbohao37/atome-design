{
  "env": {
    "browser": true,
    "es2020": true
  },
  "globals": {
    "render": "readonly"
  },
  "plugins": ["unicorn"],
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/eslint-config-typescript",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-duplicate-imports": "warn"
  }
}
