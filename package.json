{
  "name": "atome-design",
  "scripts": {
    "dev": "pnpm run swc:dev & pnpm run site:dev",
    "build": "pnpm run site:build",
    "preview": "pnpm run site:preview",
    "site:preview": "vitepress preview",
    "site:dev": "vitepress dev",
    "site:build": "vitepress build",
    "swc:build": "npx swc ./site/react-components/*.tsx -d ./site/ -C jsc.transform.react.runtime=automatic",
    "swc:dev": "pnpm run swc:build -w ",
    "commit": "git-cz",
    "prepare": "husky install",
    "release": "standard-version",
    "test:ui": "vitest --ui",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "type": "module",
  "dependencies": {
    "@arco-design/web-react": "^2.52.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "packages/*/{src,types}/**/*.ts": [
      "eslint --cache --fix"
    ],
    "packages/**/*.d.ts": [
      "eslint --cache --fix"
    ],
    "playground/**/__tests__/**/*.ts": [
      "eslint --cache --fix"
    ]
  },
  "packageManager": "pnpm@8.6.9",
  "devDependencies": {
    "@babel/parser": "^7.22.11",
    "@babel/traverse": "^7.22.11",
    "@babel/types": "^7.22.11",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@guolao/vue-monaco-editor": "^1.1.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.79",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/babel__traverse": "^7.20.1",
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-istanbul": "^0.34.4",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.3",
    "@vue/compiler-core": "^3.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "badge-maker": "^3.3.1",
    "chokidar": "^3.5.3",
    "commitizen": "^4.3.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "naive-ui": "^2.34.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^4.1.3",
    "semver": "^7.5.4",
    "standard-version": "^9.5.0",
    "typescript": "^5.1.6",
    "veaury": "^2.3.12",
    "vfonts": "^0.0.3",
    "vitepress": "1.0.0-rc.4",
    "vitest": "^0.34.3",
    "vue": "^3.3.4",
    "vue3-markdown-it": "^1.0.10"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
